{
  "projectBaseUrl":"${workspaceFolder}",
  "retryOn429": true,
  "aliveStatusCodes": [
    200,
    403
  ],
  "replacementPatterns": [
    {
      "pattern": "^/(?<path>.*)\/#.*",
      "replacement": "{{BASEURL}}/src/content/$<path>.mdx"
    },
    {
      "pattern": "^/(?<path>.*)#.*",
      "replacement": "{{BASEURL}}/src/content/$<path>.mdx"
    },
    {
      "pattern": "^/(?<path>.*)\/",
      "replacement": "{{BASEURL}}/src/content/$<path>.mdx"
    },
    {
      "pattern": "^/(?<path>.*)",
      "replacement": "{{BASEURL}}/src/content/$<path>.mdx"
    }
  ],
  "ignorePatterns": [
    { "pattern": "^#.*$" },
    { "pattern": "^{.*}$" },
    { "pattern": "^https://otlp.nr-data.net$" },
    { "pattern": "^https://insights.newrelic.com$" }
  ]
}
